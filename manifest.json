{
    "name": "Podcast Hub",
     "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "frame.html"
  },

    "version": "1",
    "manifest_version": 2,
    "content_scripts": [{
        "js": ["contentscript.js"],
        "matches": ["*://*/*"],
        "all_frames": true
    }],

    "web_accessible_resources": [
        "frame.html"
    ]
}