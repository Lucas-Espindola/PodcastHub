{
    "name": "Podcast Hub",
     "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "frame.html"
     
  },
    "version": "1",
    "manifest_version": 2,
    "permissions": [
     "activeTab",
     "tabs",
      "http://*/*", "https://*/*"
  ],
    "content_scripts": [{ 
    "matches": ["<all_urls>"],
    "js": ["jquery.js", "jquery-ui.js", "contentscript.js"]
    }],
    "web_accessible_resources": [ "frame.html"]
}